    "seed": "cross-env ELEVENTY_ENV=seed eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philhawksworth/eleventyone"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "autoprefixer": "^9.6.0",
    "axios": "^0.19.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.0.0",
    "html-minifier": "^4.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0"
  }
}
